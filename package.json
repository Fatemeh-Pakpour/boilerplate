  "description": "",
  "repository": {
    "private": true,
    "type": "git",
    "url": "git@github.com:Fatemeh-Pakpour/boilerplate.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=16.12.0"
  },
  "scripts": {
    "preinstall": "./scripts/preinstall.sh | true",
    "frontend": "cd frontend && node server.js",
    "dev": "cd frontend && next dev",
    "dev:beta": "node ./scripts/dev -beta",
    "dev:staging": "node ./scripts/dev -stagingdev",
    "backend:cloud": "./scripts/dev.sh",
    "start:backend": "cd backend && npm run start:dev",
    "start:app-frontend": "cd frontend && NODE_ENV=production node server.js",
    "build": "cd frontend && next build",
    "lint": "eslint . --ext .ts --ext .tsx --ext .mdx",
    "tsc": "tsc",
    "stylelint": "stylelint **/*.tsx",
    "unused": "ts-unused-exports tsconfig.json \"--excludePathsFromReport=pages/;apis/;.stories.tsx\"",
    "prepare": "if command -v husky; then husky install; fi",
    "build:frontends": "concurrently 'npm run build' 'npm run storybook:build' -n next,storybook -c blue,green"
  },
  "keywords": [],
  "author": "Fatemeh",
  "license": "ISC",
